		Исключение
			Сообщение = Новый СообщениеПользователю();
			Сообщение.Текст = ""Перехват клавиатуры не подключен!"";
			Сообщение.УстановитьДанные(ЭтотОбъект);
			Сообщение.Сообщить();
		КонецПопытки;
	Иначе		
		Сообщение = Новый СообщениеПользователю();
		Сообщение.Текст = ""Перехват клавиатуры не подключен!"";
		Сообщение.УстановитьДанные(ЭтотОбъект);
		Сообщение.Сообщить();
	КонецЕсли;
